{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": "^7.1.3",
        "brexis/laravel-workflow": "1.2.*",
        "coderello/laravel-nova-lang": "^1.0",
        "crabler/common": "dev-master",
        "fideloper/proxy": "^4.0",
        "laravel/framework": "5.6.*",
        "laravel/nova": "*",
        "laravel/tinker": "^1.0",
        "silvanite/novatoolpermissions": "^1.0",
        "usersettings/userprofile": "*",
        "usersettings/maincard": "*",
        "usersettings/userslist": "*",
        "usersettings/Userinformation": "*"
    },
    "require-dev": {
        "filp/whoops": "^2.0",
        "fzaninotto/faker": "^1.4",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^2.0",
        "phpunit/phpunit": "^7.0"
    },
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi",
            "@php artisan nova-lang:publish ru --force"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "repositories": [
        {
            "type": "path",
            "url": "./nova"
        },
        {
            "type": "git",
            "url": "git@bitbucket.org:opwb/common.git"
        },
        {
            "type": "path",
            "url": "./nova-components/Userprofile"
        },
        {
            "type": "path",
            "url": "./nova-components/Maincard"
        },
        {
            "type": "path",
            "url": "./nova-components/Userslist"
        },
        {
            "type": "path",
            "url": "./nova-components/Userinformation"
        }
    ]
}